<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.kuongbk.model.impl.AuthorModelImpl">
		<attributes>
			<id name="authorId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<basic name="description" />
			<basic name="born">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="male" />
			<basic name="status" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.kuongbk.model.impl.BookModelImpl">
		<attributes>
			<id name="bookId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<basic name="description" />
			<basic name="publishDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="status" />
			<basic name="viewCount" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.kuongbk.model.impl.AuthorImpl" name="Author">
		<table name="MTT_Author" />
		<attributes />
	</entity>
	<entity class="com.kuongbk.model.impl.BookImpl" name="Book">
		<table name="MTT_Book" />
		<attributes />
	</entity>
</entity-mappings>